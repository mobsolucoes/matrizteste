#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Note/Downloads/matriz-desempenho-atualizada/node_modules/.pnpm/concurrently@9.2.0/node_modules/concurrently/dist/bin/node_modules:/mnt/c/Users/Note/Downloads/matriz-desempenho-atualizada/node_modules/.pnpm/concurrently@9.2.0/node_modules/concurrently/dist/node_modules:/mnt/c/Users/Note/Downloads/matriz-desempenho-atualizada/node_modules/.pnpm/concurrently@9.2.0/node_modules/concurrently/node_modules:/mnt/c/Users/Note/Downloads/matriz-desempenho-atualizada/node_modules/.pnpm/concurrently@9.2.0/node_modules:/mnt/c/Users/Note/Downloads/matriz-desempenho-atualizada/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Note/Downloads/matriz-desempenho-atualizada/node_modules/.pnpm/concurrently@9.2.0/node_modules/concurrently/dist/bin/node_modules:/mnt/c/Users/Note/Downloads/matriz-desempenho-atualizada/node_modules/.pnpm/concurrently@9.2.0/node_modules/concurrently/dist/node_modules:/mnt/c/Users/Note/Downloads/matriz-desempenho-atualizada/node_modules/.pnpm/concurrently@9.2.0/node_modules/concurrently/node_modules:/mnt/c/Users/Note/Downloads/matriz-desempenho-atualizada/node_modules/.pnpm/concurrently@9.2.0/node_modules:/mnt/c/Users/Note/Downloads/matriz-desempenho-atualizada/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/concurrently@9.2.0/node_modules/concurrently/dist/bin/concurrently.js" "$@"
else
  exec node  "$basedir/../.pnpm/concurrently@9.2.0/node_modules/concurrently/dist/bin/concurrently.js" "$@"
fi
